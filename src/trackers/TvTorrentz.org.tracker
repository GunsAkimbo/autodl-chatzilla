<trackerinfo
	type="tvtz"
	shortName="TVTZ"
	longName="TvTorrentz.org"
	siteName="tvtorrentz.org">

	<settings>
		<cookie_description/>
		<cookie/>
	</settings>

	<servers>
		<server
			network="FreeQuest"
			serverNames="irc.freequest.net,irc.eu.freequest.net,irc.us.freequest.net"
			channelNames="#tvtorrentz_announce"
			announcerNames="TVTZ"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--NEW: Burn Notice - 3x11 - Friendly Fire (720p .mkv REPACK) (URL: http://tvtorrentz.org/details.php?id=14829)-->
				<regex value="^NEW:\s*(.*)\(URL: https?\:\/\/([^\/]+\/).*[&amp;\?]id=(\d+)"/>
				<vars>
					<var name="torrentName"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentUrl">
				<string value="http://"/>
				<var name="$baseUrl"/>
				<string value="download.php/"/>
				<var name="$torrentId"/>
				<string value="/"/>
				<varenc name="torrentName"/>
				<string value=".torrent"/>
			</var>
			<http name="cookie">
				<var name="cookie"/>
			</http>
		</linematched>
		<ignore>
		</ignore>
	</parseinfo>
</trackerinfo>
